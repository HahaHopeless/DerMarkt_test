<% if (prods.length > 0) { %>
<!-- <div class="productrow"> -->
<% for (let product of prods) { %>

<div
  class="card"
  style="
    margin-bottom: 2rem;
    max-width: 80rem;
    border-radius: 2rem;
    -webkit-box-shadow: 0px 7px 15px 0px rgba(0, 0, 0, 0.09);
    box-shadow: 0px 7px 15px 0px rgba(0, 0, 0, 0.09);
  "
>
  <div
    class="card-header"
    style="
      display: flex;
      flex-direction: row;
      align-content: space-around;
      border-top-left-radius: 2rem;
      border-top-right-radius: 2rem;
    "
  >
    <h3
      id="productDescription"
      style="font-weight: bold; margin-top: 10px; flex: 1"
    >
      <%= product.title %>
    </h3>
    <div>
      <p style="font-size: 14px">
        <i class="fa fa-clock-o" style="color: #1e90ff"></i> <%=
        product.posted_on %>
      </p>
      <div style="display: flex; flex-direction: row; float: right">
        <i class="fa fa-map-marker" style="color: #ff4757"></i>
        <p style="font-size: 14px; margin-left: 10px; margin-top: -3px">
          <%= product.city %>
        </p>
      </div>
    </div>
  </div>
  <div style="width: 100%; background-color: rgba(138, 140, 116, 0.3)">
    <div>
      <div
        id="carouselExampleIndicators"
        class="carousel slide"
        data-ride="carousel"
      >
        <ol class="carousel-indicators">
          <li
            data-target="#carouselExampleIndicators"
            data-slide-to="0"
            class="active"
          ></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" style="width: 40%; height: 300px">
          <div class="carousel-item active">
            <img
              class="d-block w-100"
              src="https://www.handyhase.de/app/uploads/2019/01/iphone-xs-max-test-1.jpg"
              alt="First slide"
            />
          </div>
          <div class="carousel-item">
            <img
              class="d-block w-100"
              src="data:image/jpeg;base64,"
              alt="Second slide"
            />
          </div>
          <div class="carousel-item">
            <img
              class="d-block w-100"
              src="https://images.indianexpress.com/2018/09/apple-iphone-xs-max-7-back2.jpg"
              alt="Third slide"
            />
          </div>
        </div>
        <a
          class="carousel-control-prev"
          href="#carouselExampleIndicators"
          role="button"
          data-slide="prev"
        >
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a
          class="carousel-control-next"
          href="#carouselExampleIndicators"
          role="button"
          data-slide="next"
        >
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
  </div>
  <div class="mt-3 tab-card">
    <div class="tab-card-header">
      <ul class="nav nav-tabs card-header-tabs" id="myTab" role="tablist">
        <li class="nav-item">
          <a
            class="nav-link active"
            id="one-tab"
            data-toggle="tab"
            href="#one"
            role="tab"
            aria-controls="One"
            aria-selected="true"
            >Description</a
          >
        </li>
        <li class="nav-item">
          <a
            class="nav-link"
            id="two-tab"
            data-toggle="tab"
            href="#two"
            role="tab"
            aria-controls="Two"
            aria-selected="false"
            >Specs</a
          >
        </li>
      </ul>
    </div>

    <div class="tab-content" id="myTabContent">
      <div
        class="tab-pane fade show active p-3"
        id="one"
        role="tabpanel"
        aria-labelledby="one-tab"
      >
        <%= product.description %>
      </div>
      <div
        class="tab-pane fade p-3"
        id="two"
        role="tabpanel"
        aria-labelledby="two-tab"
      >
        <p class="card-text">
          Color : Black <br />
          Condition : Like new <br />
        </p>
      </div>
    </div>
  </div>
  <div
    class="card-footer"
    style="
      font-weight: bolder;
      border-bottom-left-radius: 2rem;
      border-bottom-right-radius: 2rem;
      display: flex;
      flex-direction: row;
      justify-content: space-between;
      align-items: center;
      height: 6rem;
    "
  >
    <h4 style="font-weight: bold">â‚¬ <%= product.price %></h4>
    <div style="float: right">
      <button
        style="border-radius: 2rem; width: 7rem; margin-right: 1rem"
        class="btn btn-outline-primary approve"
      >
        Approve
      </button>
      <button
        style="border-radius: 2rem; width: 7rem"
        class="btn btn-outline-primary reject"
      >
        Reject
      </button>
    </div>
  </div>
</div>
<% } %>
<!-- </div> -->
<% } else { %>
<h3>Oh Snap! No Products Found!</h3>
<% } %>
